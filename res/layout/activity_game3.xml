<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF" >
	
    <view
        class="com.example.playpalpengame.DrawableRelativeLayout"
        android:id="@+id/Game3RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background" >

        <ImageView
            android:id="@+id/failFeedbackView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/fail_feedback"
            android:visibility="invisible" />
        
        <ImageView
            android:id="@+id/homeBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="25dp"
            android:src="@drawable/home" />

         <ImageView
            android:id="@+id/timeReminder"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="850dp"
            android:layout_marginTop="25dp"
            android:src="@drawable/time_reminder_1"
            android:visibility="invisible" />
         
        
         <ImageView
            android:id="@+id/Game3_bowl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:src="@drawable/game3_mixbowl" />
         
        <ImageView
            android:id="@+id/Game3_mix"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:src="@drawable/game3_mix1"
            android:visibility="visible"/>
        
        <ImageView
            android:id="@+id/Game3_mix2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:visibility="invisible"/>
        
        <ImageView
            android:id="@+id/Game3_oven"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:background="@drawable/game3_oven1"
            android:visibility="invisible"/>
        
        <ImageView
            android:id="@+id/Game3_oven2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:background="@drawable/game3_oven2"
            android:visibility="invisible"/>
        
        
        
        <ImageView
            android:id="@+id/Game3_dough"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:src="@drawable/game3_oven1"
            android:visibility="invisible" />
        
        <ImageView 
            android:id="@+id/Game3_cake"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:src="@drawable/game3_cake1"
            android:visibility="invisible"/>
         
         <ImageView 
            android:id="@+id/Game3_cakeDottedLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:src="@drawable/game3_cream_line"
            android:visibility="invisible"/>
         
         <ImageView
            android:id="@+id/Game3_cakeCream"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:src="@drawable/game3_cake_cream"
            android:visibility="invisible"/>
         
         <ImageView
            android:id="@+id/Game3_cakeStrawberry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:src="@drawable/game3_cake_strawberries"
            android:visibility="invisible"/>
         
		
         <ImageView
            android:id="@+id/Game3_cream"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/game3_cream"
            android:visibility="invisible"/>
         
         <ImageView 
            android:id="@+id/Game3_helicalView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:src="@drawable/helical_2"/>
       
        <ImageView
        	android:id="@+id/Game3_cakeCreamHintView"
        	android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="190dp"
            android:src="@drawable/game3_cake_cream_hint"
            android:visibility="invisible"/>
        
        <ImageView
            android:id="@+id/Game3_teachHand"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible" />
        	
        <TextView
            android:id="@+id/testProgressCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/boardView"
            android:layout_marginLeft="89dp"
            android:visibility="invisible"
            android:text="TextView" />
        
        <ImageView
            android:id="@+id/progressBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="350dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/progress_bottom" />

         <ProgressBar
             android:id="@+id/progressBarRed"
             style="?android:attr/progressBarStyleHorizontal"
             android:layout_width="414dip"
             android:layout_height="12dip"
             android:layout_marginLeft="373dp"
             android:layout_marginTop="61dp"
             android:max="10"
             android:maxWidth="410dp"
             android:minWidth="0dp"
             android:progress="10"
             android:progressDrawable="@drawable/progress_bar_states"
             android:visibility="visible" />

        <ImageView
            android:id="@+id/progressMark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="374dp"
            android:layout_marginTop="61dp"
            android:src="@drawable/progress_mark"
            android:visibility="visible" />    

        <ImageView
            android:id="@+id/progressBar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="350dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/progress" />
        
        <ImageView android:id="@+id/Game3_beat" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/game3_eggbeat" android:visibility="invisible" />
            
    </view>

</FrameLayout>
